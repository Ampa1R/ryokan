{
  "name": "condor",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/condor/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "dev": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "condor:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "condor:build:development"
        },
        "production": {
          "buildTarget": "condor:build:production"
        }
      }
    },
    "typeorm": {
      "command": "npx typeorm-ts-node-commonjs migration:generate foo -d ./apps/condor/src/config/db.config.ts "
    }
  }
}
